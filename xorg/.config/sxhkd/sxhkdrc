alt + d
  dmenul

alt + x
  paperlike_invert && paperlike_invert

alt + shift + x
  paperlike_invert

alt + Return
  stw_in_cwd

alt + shift + Return
  stw

alt + shift + ctrl + grave
  xdotool getactivewindow | xargs xprop -id | grep NET_WM_PID | cut -d' ' -f3 | xargs kill -9

alt + u
  externalpipe_buffer.sh dmenu_type

alt + y
  externalpipe_buffer.sh dmenu_copy

XF86Audio{RaiseVolume,LowerVolume,Mute}
  ~/.bin/voln {up,down,mute}

alt + shift + {Up,Down}
  ~/.bin/voln {up,down}

XF86MonBrightness{Up,Down}
  ~/.bin/b{up,down}

XF86Launch1
  xset dpms force suspend

alt + shift + p
  pwdhashX

alt + equal
  xdotool click --clearmodifiers 1

#~button1
#    focus.sh (underneath.sh)

alt + backslash
  urlhandler

alt + shift + backslash
  externalpipe_buffer.sh dmenu_urlhandle


