alt + Return
  urxvt

XF86Launch1
  launch goodnight

# Launcher Dmenu
alt + d
  launchit
alt + a
  cat /usr/share/foo-wm/config/commands | mdmenu foowm | xargs -0 -I INPUT foo-wm-cx /tmp/foo-wm.socket 'INPUT'
alt + space
  markselect | mdmenu jump | xargs -0 -I MARK foo-wm-cx '/tmp/foo-wm.socket' 'jump MARK'
shift + alt + p
  pwdhashX

alt + {f, shift + f}
  foo-magnify {+,-}

alt + semicolon
  foo-wm-cx /tmp/foo-wm.socket containerize

# Foo-WM Zooms
alt + {i,u}
  foo-wm-cx /tmp/foo-wm.socket 'zoom {-1,1}'

alt + {j,k,Tab,shift + Tab}
  foo-wm-cx /tmp/foo-wm.socket 'focus brother {1,-1,1,-1}'

alt + {o,p}
  foo-wm-cx /tmp/foo-wm.socket 'focus pc {-1,1}'

alt + {1-9}
  foo-wm-cx /tmp/foo-wm.socket 'set client_border_width {1-9}'

shift + alt + {j,k}
  foo-wm-cx /tmp/foo-wm.socket 'shift brother {1,-1}'

shift + alt + q
  foo-wm-cx /tmp/foo-wm.socket 'kill client'

shift + alt + z
  foo-undo

# Foo-WM Layouts

XF86Audio{RaiseVolume,LowerVolume,Mute}
  dvol -{i 2,d 2,t}

{F6, F8, XF86AudioPrev, XF86AudioNext, F7, XF86AudioPlay}
  mediaControl {previous,next,previous,next,pause,pause}

XF86Eject
  touchpad

alt + {r, shift + r}
  {surf, urxvt -e } '`sselp`'
